	<nome>Impugnar Traçado de Demarcação da SPU</nome>
	<sigla/>
	<contato/>
	<descricao>Interpor recurso em relação ao traçado da Linha do Preamar Médio - LPM e da Linha Média das Enchentes Ordinárias - LMEO em processos de demarcação da Secretaria do Patrimônio da União - SPU.</descricao>
	<gratuito>true</gratuito>
	<solicitantes>
		<solicitante>
			<tipo>Cidadãos e Famílias</tipo>
			<requisitos/>
		</solicitante>
		<solicitante>
			<tipo>Estrangeiros</tipo>
			<requisitos/>
		</solicitante>
		<solicitante>
			<tipo>Empresas privadas</tipo>
			<requisitos/>
		</solicitante>
		<solicitante>
			<tipo>Empresas públicas ou de economia mista</tipo>
			<requisitos/>
		</solicitante>
		<solicitante>
			<tipo>Pessoas jurídicas de direito privado sem fins lucrativos</tipo>
			<requisitos/>
		</solicitante>
	</solicitantes>
	<segmentos-da-sociedade>
		<item>Cidadãos</item>
		<item>Empresas</item>
		<item>Demais segmentos (ONGs, organizações sociais, etc)</item>
	</segmentos-da-sociedade>
	<tempo-total-estimado>
		<entre min="16" max="30" unidade="dias-corridos"/>
		<descricao/>
	</tempo-total-estimado>
	<etapas>
		<etapa>
			<titulo>Preencher e anexar documentos necessários ao requerimento Impugnar Traçado de Demarcação da SPU, no Portal de Atendimento da SPU.</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Carteira de identidade</item>
					<item>Carteira de identidade de estrangeiro</item>
					<item>Carteira de trabalho</item>
					<item>CNPJ</item>
					<item>Comprovante de endereço/residência</item>
					<item>CPF</item>
					<item>Procuração do representante legal</item>
					<item>Mapas e plantas do Imóvel notificado</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="web">
						<descricao>http://www.patrimoniodetodos.gov.br/#/servicoGenericoSemRip?servico=117#dados-documentos</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
		<etapa>
			<titulo>Obter o resultado da solicitação pelo e-mail informado no requerimento ou pela consulta do pedido no Portal da SPU, informando o nº do CPF ou CNPJ do requerente e o nº do atendimento.</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>CNPJ</item>
					<item>CPF</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="web">
						<descricao>www.patrimoniodetodos.gov.br</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
	</etapas>
