<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
	<nome>Utilizar Crédito para pagamento de Taxas sobre Imóvel da União</nome>
	<sigla/>
	<contato/>
	<nomes-populares>
		<item>Compensar crédito patrimonial</item>
	</nomes-populares>
	<descricao>Este serviço destina-se à compensação de créditos oriundos de receitas patrimoniais pagas a maior, em duplicidade ou indevidamente à Secretaria do Patrimônio da União.</descricao>
	<gratuito>true</gratuito>
	<solicitantes>
		<solicitante>
			<tipo>Cidadãos e Famílias</tipo>
			<requisitos/>
		</solicitante>
		<solicitante>
			<tipo>Estrangeiros</tipo>
			<requisitos/>
		</solicitante>
		<solicitante>
			<tipo>Empresas privadas</tipo>
			<requisitos/>
		</solicitante>
		<solicitante>
			<tipo>Pessoas jurídicas de direito privado sem fins lucrativos</tipo>
			<requisitos/>
		</solicitante>
	</solicitantes>
	<segmentos-da-sociedade>
		<item>Cidadãos</item>
		<item>Empresas</item>
		<item>Demais segmentos (ONGs, organizações sociais, etc)</item>
	</segmentos-da-sociedade>
	<tempo-total-estimado>
		<entre min="16" max="30" unidade="dias-corridos"/>
		<descricao/>
	</tempo-total-estimado>
	<etapas>
		<etapa>
			<titulo>Preencher o requerimento e anexar os documentos requisitados no Portal de Atendimento da SPU.</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Carteira de identidade</item>
					<item>Carteira de identidade de estrangeiro</item>
					<item>Carteira de trabalho</item>
					<item>CNPJ</item>
					<item>NIT / PIS / PASEP</item>
					<item>Cópia do DARF pago</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="web">
						<descricao>http://www.patrimoniodetodos.gov.br/#/servicoGenericoComRip?servico=116#dados-documentos-valor</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
		<etapa>
			<titulo>Obter o resultado da solicitação pelo e-mail informado no requerimento ou pela consulta do pedido no Portal da SPU, informando o nº do CPF ou CNPJ do requerente e o nº do atendimento.</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>CNPJ</item>
					<item>CPF</item>
					<item>Nº do requerimento</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="web">
						<descricao>http://www.patrimoniodetodos.gov.br/#/</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
	</etapas>
	<orgao id="http://estruturaorganizacional.dados.gov.br/id/unidade-organizacional/2981"/>
	<areas-de-interesse>
		<item>Administração </item>
	</areas-de-interesse>
	<palavras-chave/>
	<legislacoes/>
</servico>
