	</segmentos-da-sociedade>
	<tempo-total-estimado>
		<entre min="90" max="120" unidade="dias-corridos"/>
		<descricao/>
	</tempo-total-estimado>
	<etapas>
		<etapa>
			<titulo>Inscrever-se em processo seletivo.</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Carteira de identidade</item>
					<item>Carteira de trabalho</item>
					<item>CPF</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="web">
						<descricao>Página web da instituição de Ensino Superior ofertante do curso.</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
		<etapa>
			<titulo>Realizar exame de seleção.</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Carteira de identidade</item>
