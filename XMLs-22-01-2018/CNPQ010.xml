<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
	<nome>Importar bens cuja finalidade seja pesquisa científica.</nome>
	<sigla/>
	<contato/>
	<nomes-populares>
		<item>CNPq</item>
	</nomes-populares>
	<descricao>Operacionalizar importações com isenção de impostos para pesquisadores detentores de projetos aprovados pelo CNPq, que sejam credenciados junto a este Conselho.</descricao>
	<gratuito>true</gratuito>
	<solicitantes>
		<solicitante>
			<tipo>Cidadãos e Famílias</tipo>
			<requisitos/>
		</solicitante>
	</solicitantes>
	<segmentos-da-sociedade>
		<item>Cidadãos</item>
	</segmentos-da-sociedade>
	<tempo-total-estimado>
		<entre min="60" max="90" unidade="dias-corridos"/>
		<descricao/>
	</tempo-total-estimado>
	<etapas>
		<etapa>
			<titulo>Retificar documentos, prover pagemento GRU, providenciar documentos necessários para condução do processo de importação.</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Comprovante de pagamento</item>
					<item>CPF</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="email">
						<descricao>seimp@cnpq.br</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
		<etapa>
			<titulo>Enviar documentação , prover pagamento GRU.</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Comprovante de pagamento</item>
					<item>CPF</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="email">
						<descricao>seimp@cnpq.br</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
		<etapa>
			<titulo>Solicitar a execução do serviço a ser prestado.</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Comprovante de pagamento</item>
					<item>CPF</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="email">
						<descricao>seimp@cnpq.br</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
	</etapas>
	<orgao id="http://estruturaorganizacional.dados.gov.br/id/unidade-organizacional/8"/>
	<areas-de-interesse>
		<item>Pesquisa e Desenvolvimento</item>
	</areas-de-interesse>
	<palavras-chave/>
	<legislacoes/>
</servico>
