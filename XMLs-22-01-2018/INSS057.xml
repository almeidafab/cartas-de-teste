<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
	<nome>Obter Certidão por Tempo de Contribuição</nome>
	<sigla/>
	<contato/>
	<nomes-populares>
		<item>CTC</item>
	</nomes-populares>
	<descricao>Trata-se de serviço destinado ao servidor de órgão público federal, estadual, distrital ou municipal que deseja averbar o tempo de Regime Geral de Previdência Social.</descricao>
	<gratuito>true</gratuito>
	<solicitantes>
		<solicitante>
			<tipo>Empresas públicas ou de economia mista</tipo>
			<requisitos/>
		</solicitante>
		<solicitante>
			<tipo>Órgãos da administração direta</tipo>
			<requisitos/>
		</solicitante>
	</solicitantes>
	<segmentos-da-sociedade>
		<item>Empresas</item>
		<item>Órgãos e entidades públicas</item>
	</segmentos-da-sociedade>
	<tempo-total-estimado>
		<entre min="60" max="90" unidade="dias-corridos"/>
		<descricao/>
	</tempo-total-estimado>
	<etapas>
		<etapa>
			<titulo>Agendar o atendimento</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Nenhum</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="telefone">
						<descricao>135</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
		<etapa>
			<titulo>Atendimento presencial</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Carteira de identidade</item>
					<item>Carteira de trabalho</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="presencial">
						<descricao>Agência da previdência social</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
	</etapas>
	<orgao id="http://estruturaorganizacional.dados.gov.br/id/unidade-organizacional/1934"/>
	<areas-de-interesse>
		<item>Administração </item>
	</areas-de-interesse>
	<palavras-chave/>
	<legislacoes/>
</servico>
