<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
	<nome>Capacitação e Desenvolvimento de Pessoas</nome>
	<sigla/>
	<contato/>
	<descricao>Oferta de vagas para cidadãos em eventos de capacitação sobre temáticas específicas de interesse da sociedade.</descricao>
	<gratuito>true</gratuito>
	<solicitantes>
		<solicitante><tipo>Cidadãos e Famílias</tipo>
		<requisitos/>
		</solicitante>
		<solicitante><tipo>Fundações e autarquias de direito público</tipo>
		<requisitos/>
		</solicitante>
		<solicitante><tipo>Órgãos da administração direta</tipo>
		<requisitos/>
		</solicitante>
	</solicitantes>
	<segmentos-da-sociedade>
		<item>Cidadãos</item>
		<item>Órgãos e entidades públicas</item>
	</segmentos-da-sociedade>
	<tempo-total-estimado>
		<entre min="16" max="30" unidade="dias-corridos"/>
		<descricao/>
	</tempo-total-estimado>
	<etapas>
		<etapa>
			<titulo>solicitar inscrição no evento, participar do evento em si, obter certificado de participação</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Carteira de identidade</item>
					<item>carteira funcional</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="email">
						<descricao>eagu.eventos@agu.gov.br</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
		<etapa>
			<titulo>participação no evento</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Carteira de identidade</item>
					<item>carteira funcional</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="presencial">
						<descricao>Local de realização do evento, geralmente a sede da EAGU na c idade</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
		<etapa>
			<titulo>obtenção de certificado de participação</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Nenhum</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="web">
						<descricao>www.agu.gov.br/escola</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
	</etapas>
	<orgao id="http://estruturaorganizacional.dados.gov.br/id/unidade-organizacional/46"/>
	<areas-de-interesse>
		<item>Cultura</item>
