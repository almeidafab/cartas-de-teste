<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
	<nome>Inscrição, Alteração e Cancelamento de Imóvel Rural</nome>
	<sigla/>
	<contato/>
	<nomes-populares>
		<item>ITR</item>
	</nomes-populares>
	<descricao>Solicitar inscrição, alteração, cancelamento ou reativação do cadastro do imóvel rural, por meio do aplicativo Cafir – Coletor Web.</descricao>
	<gratuito>true</gratuito>
	<solicitantes>
		<solicitante><tipo>todos que possuem imóvel rural</tipo>
		<requisitos/>
		</solicitante>
	</solicitantes>
	<segmentos-da-sociedade>
		<item>Demais segmentos (ONGs, organizações sociais, etc)</item>
	</segmentos-da-sociedade>
	<tempo-total-estimado>
		<entre min="2" max="7" unidade="dias-corridos"/>
		<descricao/>
	</tempo-total-estimado>
	<etapas>
		<etapa>
			<titulo>Preencher as informações na internet, imprimir o Decir para levar a uma unidade da RFB</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Nenhum</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="web">
						<descricao>rfb.gov.br</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
		<etapa>
			<titulo>Apresentar a documentação comprobatória em uma unidade da RFB ou enviar por correio e consultar o resultado da análise da solicitação pelo Cafir Web</titulo>
			<descricao/>
			<documentos>
				<default>
					<item>Carteira de identidade</item>
					<item>Certidões de cartório</item>
					<item>Procuração do representante legal</item>
					<item>Registro em Cartório</item>
				</default>
			</documentos>
			<custos>
				<default/>
			</custos>
			<canais-de-prestacao>
				<default>
					<canal-de-prestacao tipo="presencial">
						<descricao>CAC da RFB ou Correios</descricao>
					</canal-de-prestacao>
					<canal-de-prestacao tipo="postal">
						<descricao>Unidade de jurisdição do contribuinte</descricao>
					</canal-de-prestacao>
				</default>
			</canais-de-prestacao>
		</etapa>
	</etapas>
	<orgao id="http://estruturaorganizacional.dados.gov.br/id/unidade-organizacional/1929"/>
	<areas-de-interesse>
		<item>Administração </item>
	</areas-de-interesse>
	<palavras-chave/>
	<legislacoes/>
</servico>
